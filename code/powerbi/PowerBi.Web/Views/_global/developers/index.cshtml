@model PowerBI.Web.Models.Pages.DevelopersViewModel
@using Resources = PowerBI.Resources.Pages.Developers.Index
@{
    Model.PageTitle = Resources.META_PAGETITLE;
    Model.MetaDescription = Resources.META_DESCRIPTION;
    Model.MetaKeywords = Resources.META_KEYWORDS;
}

<section class="section section-size2 section-palette02 section-heading-only">
    <div class="row">
        <div class="column medium-8">
            <h1>@Resources.Hero_Heading</h1>
        </div>
        <div class="column medium-4">
            <form action="https://social.msdn.microsoft.com/Search/" method="get" onsubmit="event.stopPropagation();">
                <div class="search-input search-input-small" style="margin-top: 12px">
                    <input type="text" name="query" placeholder="@Resources.SearchPlaceholder" />
                    <button type="submit" class="icon">@SvgHelper.GetSvg("search")</button>
                </div>
            </form>
        </div>
    </div>
</section>

<section class="section section-slate02 section-developer">
    <div class="row">
        <div class="column large-6">
            <div class="row column">
                <h2>@Resources.API_Heading</h2>
                <p>@Resources.API_Content</p>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a href="@Html.LocalizeUrl("go:746470")" class="button button-featured">@Resources.API_CTA_GetStartedNow</a>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="section">
    <div class="row">
        <div class="column medium-3">
            <div class="row column">
                <span class="icon icon-palette02 icon-size3">@SvgHelper.GetSvg("real-time")</span>
            </div>
            <div class="row row-size1 sd-equalize" data-set="capabilities">
                <div class="column">
                    <h3>@Resources.Embed_Heading</h3>
                    <p>@Resources.Embed_Content</p>
                </div>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a class="button" href="@Html.LocalizeUrl("go:746470")">@Resources.Embed_CTA_GetStarted</a>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                <span class="icon icon-palette02 icon-size3">@SvgHelper.GetSvg("data")</span>
            </div>
            <div class="row row-size1 sd-equalize" data-set="capabilities">
                <div class="column">
                    <h3>@Resources.Visualize_Heading</h3>
                    <p>@Resources.Visualize_Content</p>
                </div>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a class="button" href="@Html.LocalizeUrl("/custom-visuals/")">@Resources.Extend_CTA_LearnMore</a>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                <span class="icon icon-palette02 icon-size3">@SvgHelper.GetSvg("connection")</span>
            </div>
            <div class="row row-size1 sd-equalize" data-set="capabilities">
                <div class="column">
                    <h3>@Resources.ConnectAndAnalyze_Heading</h3>
                    <p>@Resources.ConnectAndAnalyze_Content</p>
                </div>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a class="button" href="@Html.LocalizeUrl("/developers/#contentPacks")">@Resources.Extend_CTA_LearnMore</a>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                 <span class="icon icon-palette02 icon-size3">@SvgHelper.GetSvg("integrate")</span>
            </div>
            <div class="row row-size1 sd-equalize" data-set="capabilities">
                <div class="column">
                    <h3>@Resources.Connect_Heading</h3>
                    <p>@Resources.Connect_Content</p>
                </div>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a class="button" href="#connect">@Resources.Connect_CTA_LearnMore</a>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="section section-slate09">
    <div class="row">
        <div class="column medium-6">
            <h3>@Resources.DevelopersPlaylist_Heading</h3>
            <iframe class="sd-equalize" data-set="media" width="95%" height="315" src="https://www.youtube.com/embed/videoseries?list=PL1N57mwBHtN12K0rY9QkqAX4OUqw7azi8" frameborder="0" allowfullscreen></iframe>
            <p><a target="_blank" class="text-mini" href="https://www.youtube.com/channel/UCy--PYvwBwAeuYaR8JLmrfg">@Resources.DevelopersPlaylist_CTA</a></p>
        </div>
        <div class="column medium-5">
            <h3>@Resources.DevelopersBlog_Heading</h3>
            <div class="sd-equalize" data-set="media">
                @if (Model.BlogPosts != null)
                {
                foreach (var post in Model.BlogPosts)
                {
                <article class="row row-size1">
                    <div class="column">
                        <span><a href="@Html.LocalizeUrl(post.Url)">@post.Title</a></span><br />
                        <span class="text-mini">@post.Published.ToString("D")</span>
                    </div>
                </article>
                }
                }
            </div>
            <p><a class="text-mini" href="@Html.LocalizeUrl("/blog/")">@Resources.DevelopersBlog_CTA_ViewBlog</a></p>

        </div>
    </div>
</section>

<section class="section section-slate01 text-center" id="contentPacks">
    <div class="row">
        <div class="column medium-10 medium-offset-1">
            <div class="row column">
                <h2 class="text-palette02">@Resources.Connectors_Heading</h2>
                <p>@Resources.Connectors_Content1</p>
                <p>@Resources.Connectors_Content2</p>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <a href="https://azure.microsoft.com/marketplace/programs/certified/apply/" class="button button-featured">@Resources.Connectors_CTA_ContactUs</a>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="section section-slate06 text-center" id="connect">
    <div class="row">
        <div class="column medium-3">
            <div class="row column">
                <span class="icon">@SvgHelper.GetSvg("desktop")</span>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <h3>@Resources.ClientApp_Heading</h3>
                    <ul class="linkList">
                        <li><a href="https://dev.powerbi.com/apps?type=native">@Resources.ClientApp_CTA_RegisterYourApp</a></li>
                        <li><a href="@Html.LocalizeUrl("go:619653")">@Resources.ClientApp_CTA_Documentation</a></li>
                        <li><a href="https://github.com/Microsoft/PowerBI-CSharp/tree/master/samples/consoleapp/getting-started-for-dotnet">@Resources.ClientApp_CTA_Sample</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                <span class="icon">@SvgHelper.GetSvg("web")</span>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <h3>@Resources.Web_Heading</h3>
                    <ul class="linkList">
                        <li><a href="https://dev.powerbi.com/apps?type=web">@Resources.Web_CTA_RegisterYourApp</a></li>
                        <li><a href="@Html.LocalizeUrl("go:619654")">@Resources.Web_CTA_Documentation</a></li>
                        <li><a href="https://github.com/PowerBI/getting-started-web-app-asp.net/tree/master">@Resources.Web_CTA_Sample</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                <span class="icon">@SvgHelper.GetSvg("real-time")</span>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <h3>@Resources.RestApiRealTime_Heading</h3>
                    <ul class="linkList">
                        <li><a href="http://docs.powerbi.apiary.io/">@Resources.RestApiRealTime_CTA_TryTheApi</a></li>
                        <li><a href="https://azure.microsoft.com/documentation/articles/stream-analytics-power-bi-dashboard/">@Resources.RestApiRealTime_CTA_AzureStreamAnalytics</a></li>
                        <li><a href="@Html.LocalizeUrl("go:618289")">@Resources.RestApiRealTime_CTA_Documentation</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="column medium-3">
            <div class="row column">
                <span class="icon">@SvgHelper.GetSvg("extend")</span>
            </div>
            <div class="row row-size2">
                <div class="column">
                    <h3>@Resources.ExtendPowerBi_Heading</h3>
                    <ul class="linkList">
                        <li><a href="@Html.LocalizeUrl("/custom-visuals/")">@Resources.ExtendPowerBi_CTA_CustomVisuals</a></li>
                        <li><a href="@Html.LocalizeUrl("go:618345")">@Resources.ExtendPowerBi_CTA_SeeOnGithub</a></li>
                        <li><a href="@Html.LocalizeUrl("go:618346")">@Resources.ExtendPowerBi_CTA_Documentation</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="section section-slate05">
    <div class="row">
        <div class="column medium-6">
            <h5>@Resources.Documentation_Heading</h5>
            <div class="row">
                <div class="column small-6">
                    <ul class="linkList text-heading4">
                        <li><a href="@Html.LocalizeUrl("msdn:dn877544")">@Resources.Documentation_CTA_Documentation</a></li>
                        <li><a href="@Html.LocalizeUrl("go:619655")">@Resources.Documentation_CTA_GettingStarted</a></li>
                        <li><a href="@Html.LocalizeUrl("msdn:dn877545")">@Resources.Documentation_CTA_StartingWithOauth</a></li>
                    </ul>
                </div>
                <div class="column small-6">
                    <ul class="linkList text-heading4">
                        <li><a href="@Html.LocalizeUrl("msdn:dn877542")">@Resources.Documentation_CTA_RegisterAnApp</a></li>
                        <li><a href="@Html.LocalizeUrl("msdn:dn889826")">@Resources.Documentation_CTA_PowerBiApiTerms</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="column medium-6">
            <h5>@Resources.LearnMore_Heading</h5>
            <div class="row">
                <div class="column small-6">
                    <ul class="linkList text-heading4">
                        <li><a href="http://docs.powerbi.apiary.io/">@Resources.LearnMore_CTA_ApiReference</a></li>
                        <li><a href="http://stackoverflow.com/questions/tagged/powerbi">@Resources.LearnMore_CTA_Community</a></li>
                        <li><a href="http://support.powerbi.com/forums/268152-developer-apis">@Resources.LearnMore_CTA_Feedback</a></li>
                    </ul>
                </div>
                <div class="column small-6">
                    <ul class="linkList text-heading4">
                        <li><a href="@Html.LocalizeUrl("/blog/")">@Resources.LearnMore_CTA_DevBlog</a></li>
                        <li><a href="https://github.com/powerbi">@Resources.LearnMore_CTA_Samples</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
